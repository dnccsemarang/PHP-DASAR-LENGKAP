{
    "name": "rizdev/belajar-php-unit-test",
    "description": "Belajar PHP Unit Test",
    "type": "project",
    "authors": [
        {
            "name": "Rizki Darmawan",
            "email": "rizkidarmawan.0402102@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "rizdev\\Test\\" : "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "rizdev\\Test\\" : "test"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit --configuration phpunit.xml",
        "test:product": "vendor/bin/phpunit --configuration phpunit.xml --testsuite product"
    },
    "require": {
        "php": "7.4.*"
    },
    "require-dev": {
        "phpunit/phpunit": "9.5.*"
    }
}
